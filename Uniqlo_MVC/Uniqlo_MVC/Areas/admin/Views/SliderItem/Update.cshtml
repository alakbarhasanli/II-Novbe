@using Uniqlo_MVC.Models
@model SliderItem

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Title"></label>
        <input class="form-control" asp-for="Title">
        <span asp-validation-for="Title" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for=" MainImageUrl "></label>
        <input class="form-control" asp-for=" MainImageUrl ">
        <span asp-validation-for="MainImageUrl" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ButtonUrl"></label>
        <input class="form-control" asp-for="ButtonUrl">
        <span asp-validation-for="ButtonUrl" class="text-danger"></span>

    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>